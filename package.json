{
  "name": "vocabulary.training",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@firebase/app-types": "0.1.0",
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-regular": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0",
    "firebase": "^4.12.1",
    "history": "^4.7.2",
    "immutability-helper": "^2.6.6",
    "jquery": "^3.0.0",
    "moment": "^2.22.0",
    "popper.js": "^1.12.9",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-flag-icon-css": "^1.0.23",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.1",
    "react-textfit": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/styles/sass -o ./src/styles/css --output-style expanded -x",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles/sass -o ./src/styles/css --output-style expanded -x --watch --recursive",
    "build-css-prod": "node-sass-chokidar ./src/styles/sass -o ./src/styles/css --output-style compressed -x",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css-prod build-js",
    "test": "react-scripts test --env=jsdom",
    "deploy": "run-s deploy-deploy deploy-login",
    "deploy-deploy": "firebase login",
    "deploy-login": "firebase deploy",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2"
  }
}
